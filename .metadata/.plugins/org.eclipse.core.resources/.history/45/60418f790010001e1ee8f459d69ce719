package ex0621;

public class Ex05형변환2 {

	public static void main(String[] args) {
		
		// 정수형 : byte, int
		
		byte num1 = 3;
		int num2 = 8000;
		
		num1 = (byte)num2;
		System.out.println(num1);
		
		int num3 = 1000;
		
		num1 = (byte)num3;
		System.out.println(num1);
		
		
		
		
		
		
		
		
		
		
		
		

	}

}
