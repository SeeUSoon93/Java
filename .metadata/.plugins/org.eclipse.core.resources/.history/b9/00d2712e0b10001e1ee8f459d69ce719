package ex0621;

import java.util.Scanner;

public class Ex08산술연산자3 {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		int num = sc.nextInt();
		
		System.out.println(num/100 + "00");
		System.out.println(num - (num%100));
		System.out.println(num/100 + "00");
		
		
		
		
		
		
	}

}
