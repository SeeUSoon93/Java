package ex0621;

public class Ex04형변환1 {

	public static void main(String[] args) {
		
		// int(4byte) -> double(8byte)
		int a = 3;
		System.out.println("a 값 : " + a);
		
		double b = a;
		System.out.println("b 값 : " + b);
		
		// double -> int
		double c = 45.9;
		System.out.println("c 값 : " + c);
		
		int d = (int)c;
		System.out.println("d 갑 : " + d);
		
		
		
		
		
		
		
		
	}

}
