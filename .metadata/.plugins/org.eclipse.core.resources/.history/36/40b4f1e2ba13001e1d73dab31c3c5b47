package while문;

import java.util.Scanner;

public class Ex05실습5 {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		int num = sc.nextInt();
		
		do {
			System.out.println(">" + num);
		}while(num == 0); {
			System.out.println("프로그램 종료");
		}
		
		
		
		
		
		
		
	}

}
