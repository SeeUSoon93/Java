import java.util.Scanner;

public class Ex05 {

	public static void main(String[] args) {

		// A,B 숫자를입력받는다.
		// A-B를출력한다. 
		// A와B가모두0을입력받으면프로그램이종료된다.
		
		Scanner sc = new Scanner(System.in);
		System.out.print("A 입력 >> ");
		int a = sc.nextInt();
		System.out.print("B 입력 >> ");
		int b = sc.nextInt();
		
		while(true) {
			
			System.out.print("결과 >> " + (a-b));
			
			if(a == 0 && b == 0) {
				System.out.println("종료.");
				break;
	}

}
