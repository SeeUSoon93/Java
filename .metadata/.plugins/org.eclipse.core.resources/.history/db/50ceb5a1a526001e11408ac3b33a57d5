						+ "⠀⠀⠀⠀⠀⠀⠀⠀⠀⠀⠀⠀⠀⠀⠀⠀⠀⠀⠀⠀⠀⠀⠀⠀⠀⠀⠀⠀⠀⠀⠀⠀⠀⠀⠀⠀⠀⠀⠀⠀⠀\r\n" + "",
				"4", "4", "5", "5", "7", "7", "8", "9", "9" };
		Random rd = new Random();
		int a = rd.nextInt(roulette1.length);
		int b = rd.nextInt(roulette2.length);
		int c = rd.nextInt(roulette3.length);

		System.out.print(roulette1[a] + "\t");
		try {
			Thread.sleep(1000);
			System.out.print(roulette2[b] + "\t");
			Thread.sleep(1000);
		} catch (InterruptedException e) {
			e.printStackTrace();
		}
		System.out.print(roulette3[c] + "\t");

	}
}
