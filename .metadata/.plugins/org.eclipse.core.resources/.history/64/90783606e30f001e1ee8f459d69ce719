package ex0621;

public class Ex01변수 {

	public static void main(String[] args) {
		
		// 변수 선언, 할당(초기화), 재할당
		// 1. 변수 선언
		int num;
		
		// 2. 할당(초기화)
		num = 10;
		System.out.println(num);
		
		// 3. 재할당
		num = 20;
		System.out.println(num);
		
		// 4. 선언하면서 초기화하기
		int num2 = 30;
		System.out.println(num2);
		

	}

}
